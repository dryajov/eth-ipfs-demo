{
  "name": "transfer-files",
  "version": "1.0.0",
  "scripts": {
    "check-bundle": "test -f ../../../dist/index.js && npm run copy-bundle || (echo \"js-ipfs dist file not found, go up two dirs and run 'npm run build' first\" && exit 1)",
    "copy-bundle": "cp ../../../dist/index.js ./js/ipfs.js",
    "serve": "http-server -c-1 -p 12345",
    "start": "beefy js/app.js --live --open -- -r browserify-zlib-next:zlib"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2017"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "http-server": "^0.9.0"
  },
  "dependencies": {
    "babel-preset-es2017": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify-zlib-next": "^1.0.1",
    "eth-block-tracker": "^1.0.1",
    "ethjs-provider-http": "^0.1.6",
    "ipfs": "^0.21.8",
    "obs-store": "^2.3.2",
    "virtual-dom": "^2.1.1"
  }
}
